<h1>Listing Services</h1>

<table class="table table-striped">
  <tr>
    <th>Name</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @services.each do |service| %>
  <tr>
    <td><%= service.name %></td>
    <td><%= link_to '',  service, :class => "icon-eye-open" %></td>
    <td><%= link_to '', edit_service_path(service), :class => "icon-edit" %></td>
    <td><%= link_to '', service, :class => "icon-remove", method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />
<%= button_to 'New Service', new_service_path, :class => "btn  btn-primary", :method => :get %>
